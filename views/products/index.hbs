{{#extends 'base'}}
{{#block 'content'}}
<table class="table">
    <thead>
        <tr>
            <th>name</th>
            <th>description</th>
            <th>cost</th>
        </tr>
    </thead>
    <tbody>
        {{#each products}}
        <tr>
            <td>{{this.name}}</td>
            <td>{{this.description}}</td>
            <td>{{this.cost}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>
{{/block}}
{{/extends}}